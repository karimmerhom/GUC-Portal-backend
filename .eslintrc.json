{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "nonblock-statement-body-position": "off",
    "linebreak-style": "off",
    "curly": "off",
    "object-curly-newline": "off",
    "operator-linebreak": "off",
    "no-underscore-dangle": "off",
    "comma-dangle": "off",
    "semi": "off",
    "camelcase": "off",
    "arrow-parens": "off"
  }
}
